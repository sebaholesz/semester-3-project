@section Stylesheets {
    <link href="~/css/manage-user.css" rel="stylesheet" />
}

@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}
<main>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12 screen-1300-full">
                <div class="d-block boxed-row h-100 w-100">
                    <h2 class="text-center">Manage your account</h2>
                    <hr />  
                    <div class="row">
                        <div class="col-md-3">
                            <partial name="_ManageNav" />
                        </div>
                        <div class="col-md-1">
                        </div>
                        <div class="col-md-8">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>


@section Scripts {
    @RenderSection("Scripts", required: false)
}
